{
  "name": "project-one",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:href": "ng build --base-href ./",
    "delete:docs": "del docs",
    "copy:dist": "copyfiles dist/project-one/browser/* ./docs -f",
    "copy:dist:assets:img": "copyfiles dist/project-one/browser/assets/img/* ./docs/assets/img -f",
    "copy:dist:assets:json": "copyfiles dist/project-one/browser/assets/json/* ./docs/assets/json -f",
    "build:github": "npm run delete:docs && npm run build:href && npm run copy:dist && npm run copy:dist:assets:img && npm run copy:dist:assets:json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "copyfiles": "^2.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "list-files-in-dir": "^0.1.5",
    "typescript": "~5.3.2"
  }
}
